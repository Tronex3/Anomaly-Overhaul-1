[awr_mode]
; Режим вывода отладочной информации
debug_mode = false
; Режим вывода содержимого таблицы AWR из Store в лог, при каждом обращении к Store
debug_show_tables = false
; Отладочный режим тестирования мешей (последовательное переключение мешей по списку из секции awr_test)
mesh_test_mode = false

[awr_settings]
; Задержка, по прошествии которой, оставленное в тисках оружие может быть украдено
steal_delay = 18000
; Перманентная прибавка к шансу получения детали при разборке, в формуле wpn_cond*100+x >= math.random(1,100)
condition = 40
outfit_condition = 10
; Расстояние до механика при проверки дальности в функции выбора угла
distance = 40
; Количество деталей каждого типа (должно точно совпадать с реальным)
bolt 			= 17
barrel 			= 12
gas_tube 		= 11
bolt_carrier 	= 11
trigger 		= 6

