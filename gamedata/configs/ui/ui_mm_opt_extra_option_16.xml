<w>
	<background width="1024" height="768">
	    <auto_static x="0" y="0" width="1024" height="768" stretch="1">
			<texture x="0" y="1024" width="1280" height="768">ui\ui_actor_main_menu_one</texture>
		</auto_static>
	</background>

	<main_dialog>
		<dialog x="159" y="185" width="515" height="397" stretch="1">
			<texture>ui_new_game_popup_option</texture>
		</dialog>

		<btn_cancel x="114" y="343" width="69" height="24" stretch="1">
            <text align="c" font="letterica18">kb_quit</text>
            <texture>ui_inGame2_button</texture>
		</btn_cancel>
		
		<btn_default x="191" y="343" width="69" height="28" stretch="1">
            <text font="letterica18" align="c">ui_st_reset</text>
            <texture>ui_inGame2_button</texture>
		</btn_default>
		
		<btn_accept x="268" y="343" width="69" height="24" stretch="1">
            <text align="c" font="letterica18">ui_mm_apply</text>
            <texture>ui_inGame2_button</texture>
		</btn_accept>

		<cap_opt x="15" y="8" width="205" height="27">
			<text complex_mode="1" r="250" g="250" b="250" align="c" vert_align="c" font="letterica18" />
		</cap_opt>
	</main_dialog>

	<tab_size x="18" y="50" width="458" height="282"/>

    <tab_gameplay>
		<cap_check_axr x="100" y="3" width="108" height="24">
			<text r="170" g="170" b="170" font="letterica16" align="l" vert_align="c"></text> <!-- check - gray -->
		</cap_check_axr>
	
		<check_axr x="355" y="5" width="35" stretch="1" height="29">
				<texture>ui_trx_menu_opt_checkbox3</texture>
		</check_axr>
	
		<cap_list_axr x="175" y="3" width="108" height="24">
			<text r="170" g="170" b="170" font="letterica16" align="r" vert_align="c"></text> <!-- list - gray -->
		</cap_list_axr>
	
		<list_axr x="290" y="5" width="110" height="20">
			<list_font r="170" g="170" b="170" font="letterica16"/>
			<text_color>
				<e r="170" g="170" b="170"/>
				<d r="70" g="70" b="70"/>
			</text_color>
		</list_axr>
	
		<cap_edit_axr x="60" y="3" width="108" height="24">
			<text r="170" g="170" b="170" font="letterica16" align="l" vert_align="c"></text> <!-- edit box - gray -->
		</cap_edit_axr>
	
		<edit_axr x="325" y="5" width="80" height="20" align="l">
			<text x="5" font="letterica16" vert_align="c" align="1"/>
			<auto_static x="0" y="0" width="80" height="20" stretch="1">
				<texture a="255" r="26" g="34" b="26">ui\ui_console</texture>
			</auto_static>
		</edit_axr>
	
		<cap_text_axr x="160" y="9" width="108" height="24">
			<text r="100" g="100" b="250" font="letterica16" align="c" vert_align="c"></text> <!-- text - blue -->
		</cap_text_axr>
			
		<templ_item width="458" height="30"/>
		<scroll_v x="0" y="0" width="458" height="282" right_ident="0" left_ident="0" top_indent="0" bottom_indent="0" vert_interval="0" always_show_scroll="1"/>
    </tab_gameplay>
</w>
